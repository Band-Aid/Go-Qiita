{
    "name": "go-qiita",
    "displayName": "go Qiita",
    "description": "upload new article to Qiita",
    "version": "0.0.1",
    "publisher": "Daichi",
    "engines": {
        "vscode": "^1.21.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.launch"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.launch",
                "title": "launchqiita"
            },
            {
                "command": "extension.createTemplate",
                "title": "template"
            },
            {
                "command": "extension.post",
                "title": "post"
            },
            {
                "command": "extension.update",
                "title": "update"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.61",
        "eslint": "^4.11.0",
        "typescript": "^2.8.3",
        "vscode": "^1.1.16"
    },
    "dependencies": {
        "hoek": "^4.2.1",
        "node-rest-client": "^3.1.0"
    }
}
